{
  "openapi": "3.0.2",
  "paths": {
    "/dataset/{datasetId}/locations": {
      "get": {
        "description": "Obtains all the locations for a dataset by the dataset ID, which by passing in the appropriate options, might be in different formats",
        "summary": "obtains a dataset's locations",
        "tags": [],
        "parameters": [
          {
            "name": "datasetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[A-Za-z0-9_-]+$"
            },
            "description": "uniquely specifies the dataset wanted"
          }
        ],
        "operationId": "getDatasetLocations",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    },
                    "minItems": 2,
                    "maxItems": 2
                  },
                  "description": "the dataset matching the supplied ID"
                }
              }
            }
          },
          "400": {
            "description": "400",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          },
          "404": {
            "description": "404",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          }
        }
      }
    },
    "/dataset/{datasetId}/search": {
      "get": {
        "description": "Obtains an array of dataset item indexes, which match the search criteria supplied",
        "summary": "obtains a list of dataset entries satisfying the search criteria supplied",
        "tags": [],
        "parameters": [
          {
            "name": "datasetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[A-Za-z0-9_-]+$"
            },
            "description": "uniquely specifies the dataset wanted"
          },
          {
            "name": "text",
            "in": "query",
            "description": "a text fragment to match",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "filter",
            "in": "query",
            "description": "uniquely specifies the taxonomy filter items wanted",
            "schema": {
              "oneOf": [
                {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                {
                  "type": "string"
                }
              ]
            }
          }
        ],
        "operationId": "searchDataset",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "description": "the dataset item indexes matching the supplied criteria"
                }
              }
            }
          },
          "400": {
            "description": "400",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          },
          "404": {
            "description": "404",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          }
        }
      }
    },
    "/dataset/{datasetId}/item/{datasetItemIdOrIx}": {
      "get": {
        "description": "Obtains a single dataset item by its ID or its index, and the dataset's ID.",
        "summary": "obtains a dataset item by its unique ID",
        "tags": [],
        "parameters": [
          {
            "name": "datasetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[A-Za-z0-9_-]+$"
            },
            "description": "uniquely specifies the dataset wanted"
          },
          {
            "name": "datasetItemIdOrIx",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "uniquely specifies the dataset item wanted within the dataset"
          }
        ],
        "operationId": "getDatasetItem",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {},
                  "additionalProperties": true,
                  "description": "the dataset item matching the supplied ID or index"
                }
              }
            }
          },
          "400": {
            "description": "400",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          },
          "404": {
            "description": "404",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          }
        }
      }
    },
    "/dataset/{datasetId}/config": {
      "get": {
        "description": "Obtains configured parameters for a map, which amongst other things, include default values for various options, and definitions of vocabulary terms with their localised labels, that are used to interpret identifers in the data and/or elsewhere.",
        "summary": "obtain various configured parameters for a map",
        "tags": [],
        "parameters": [
          {
            "name": "datasetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[A-Za-z0-9_-]+$"
            },
            "description": "uniquely specifies the dataset wanted"
          }
        ],
        "operationId": "getConfig",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "prefixes": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string"
                      }
                    },
                    "vocabs": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "object",
                        "additionalProperties": {
                          "type": "object",
                          "properties": {
                            "title": {
                              "type": "string"
                            },
                            "terms": {
                              "type": "object",
                              "additionalProperties": {
                                "type": "string"
                              }
                            }
                          },
                          "required": [
                            "title",
                            "terms"
                          ]
                        }
                      }
                    },
                    "itemProps": {
                      "type": "object",
                      "additionalProperties": {
                        "discriminator": {
                          "propertyName": "type"
                        },
                        "oneOf": [
                          {
                            "type": "object",
                            "properties": {
                              "titleUri": {
                                "type": "string"
                              },
                              "filter": {
                                "type": "boolean"
                              },
                              "type": {
                                "type": "string",
                                "enum": [
                                  "value"
                                ]
                              },
                              "as": {
                                "type": "string",
                                "enum": [
                                  "string",
                                  "boolean",
                                  "number"
                                ]
                              },
                              "strict": {
                                "type": "boolean"
                              }
                            },
                            "required": [
                              "type"
                            ]
                          },
                          {
                            "type": "object",
                            "properties": {
                              "titleUri": {
                                "type": "string"
                              },
                              "filter": {
                                "type": "boolean"
                              },
                              "type": {
                                "type": "string",
                                "enum": [
                                  "vocab"
                                ]
                              },
                              "uri": {
                                "type": "string"
                              }
                            },
                            "required": [
                              "type",
                              "uri"
                            ]
                          },
                          {
                            "type": "object",
                            "properties": {
                              "titleUri": {
                                "type": "string"
                              },
                              "filter": {
                                "type": "boolean"
                              },
                              "type": {
                                "type": "string",
                                "enum": [
                                  "multi"
                                ]
                              },
                              "of": {
                                "oneOf": [
                                  {
                                    "type": "object",
                                    "properties": {
                                      "type": {
                                        "type": "string",
                                        "enum": [
                                          "value"
                                        ]
                                      },
                                      "as": {
                                        "type": "string",
                                        "enum": [
                                          "string",
                                          "boolean",
                                          "number"
                                        ]
                                      },
                                      "strict": {
                                        "type": "boolean"
                                      }
                                    },
                                    "required": [
                                      "type"
                                    ]
                                  },
                                  {
                                    "type": "object",
                                    "properties": {
                                      "type": {
                                        "type": "string",
                                        "enum": [
                                          "vocab"
                                        ]
                                      },
                                      "uri": {
                                        "type": "string"
                                      }
                                    },
                                    "required": [
                                      "type",
                                      "uri"
                                    ]
                                  }
                                ]
                              }
                            },
                            "required": [
                              "type",
                              "of"
                            ]
                          }
                        ]
                      }
                    }
                  },
                  "required": [
                    "prefixes",
                    "vocabs",
                    "itemProps"
                  ],
                  "description": "variuos configured parameters for a map"
                }
              }
            }
          },
          "400": {
            "description": "400",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          },
          "404": {
            "description": "404",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true,
                  "description": "no such map"
                }
              }
            }
          }
        }
      }
    },
    "/version": {
      "get": {
        "description": "Obtains version information about the backend Mykomap server, in the form of a JSON object",
        "summary": "obtains Mykomap server version information",
        "tags": [],
        "parameters": [],
        "operationId": "getVersion",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "buildTime": {
                      "type": "string",
                      "format": "date-time"
                    },
                    "version": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    },
                    "commitDesc": {
                      "type": "string"
                    },
                    "nodeEnv": {
                      "type": "string",
                      "enum": [
                        "production",
                        "development"
                      ]
                    }
                  },
                  "required": [
                    "name",
                    "buildTime",
                    "version",
                    "commitDesc",
                    "nodeEnv"
                  ],
                  "description": "information about the current Mykomap server version"
                }
              }
            }
          }
        }
      }
    }
  },
