{
  "openapi": "3.0.2",
  "paths": {
    "/dataset/{datasetId}": {
      "get": {
        "description": "Obtains a dataset by its ID, which by passing in the appropriate options, might be in different formats",
        "summary": "obtains a dataset",
        "tags": [],
        "parameters": [
          {
            "name": "datasetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[A-Z0-9_-]+$"
            }
          }
        ],
        "operationId": "getDataset",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    },
                    "minItems": 2,
                    "maxItems": 2
                  }
                }
              }
            }
          },
          "400": {
            "description": "400",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true
                }
              }
            }
          },
          "404": {
            "description": "404",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true
                }
              }
            }
          }
        }
      }
    },
    "/dataset/{datasetId}/search": {
      "get": {
        "description": "Obtains an array of dataset item IDs, which match the search criteria supplied",
        "summary": "obtains a list of dataset entries satisfying the search criteria supplied",
        "tags": [],
        "parameters": [
          {
            "name": "datasetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[A-Z0-9_-]+$"
            }
          },
          {
            "name": "text",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "filter",
            "in": "query",
            "schema": {
              "oneOf": [
                {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "pattern": "^(?:[_\\p{Ll}\\p{Lu}\\p{Lt}\\p{Lo}\\p{Nl}][_.\\p{L}\\p{M}\\p{Nd}\\p{Nl}-]*)[:](?:[_\\p{Ll}\\p{Lu}\\p{Lt}\\p{Lo}\\p{Nl}][_.\\p{L}\\p{M}\\p{Nd}\\p{Nl}-]*)$"
                  }
                },
                {
                  "type": "string",
                  "pattern": "^(?:[_\\p{Ll}\\p{Lu}\\p{Lt}\\p{Lo}\\p{Nl}][_.\\p{L}\\p{M}\\p{Nd}\\p{Nl}-]*)[:](?:[_\\p{Ll}\\p{Lu}\\p{Lt}\\p{Lo}\\p{Nl}][_.\\p{L}\\p{M}\\p{Nd}\\p{Nl}-]*)$"
                }
              ]
            }
          }
        ],
        "operationId": "searchDataset",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "integer",
                    "minimum": 0,
                    "nullable": true
                  }
                }
              }
            }
          },
          "400": {
            "description": "400",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true
                }
              }
            }
          },
          "404": {
            "description": "404",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true
                }
              }
            }
          }
        }
      }
    },
    "/dataset/{datasetId}/item/{datasetItemId}": {
      "get": {
        "description": "Obtains a single dataset item by its ID and the dataset's ID.",
        "summary": "obtains a dataset item by its unique ID",
        "tags": [],
        "parameters": [
          {
            "name": "datasetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "pattern": "^[A-Z0-9_-]+$"
            }
          },
          {
            "name": "datasetItemId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "minimum": 0,
              "nullable": true
            }
          }
        ],
        "operationId": "getDatasetItem",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {},
                  "additionalProperties": true
                }
              }
            }
          },
          "400": {
            "description": "400",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true
                }
              }
            }
          },
          "404": {
            "description": "404",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ],
                  "additionalProperties": true
                }
              }
            }
          }
        }
      }
    },
    "/version": {
      "get": {
        "description": "Obtains version information about the backend Mykomap server, in the form of a JSON object",
        "summary": "obtains Mykomap server version information",
        "tags": [],
        "parameters": [],
        "operationId": "getVersion",
        "responses": {
          "200": {
            "description": "200",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "buildTime": {
                      "type": "string",
                      "format": "date-time"
                    },
                    "version": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    },
                    "commitDesc": {
                      "type": "string"
                    },
                    "nodeEnv": {
                      "type": "string",
                      "enum": [
                        "production",
                        "development"
                      ]
                    }
                  },
                  "required": [
                    "name",
                    "buildTime",
                    "version",
                    "commitDesc",
                    "nodeEnv"
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
