          "AM50": "Energy",
          "AM60": "Food",
          "AM70": "Goods & Services",
          "AM80": "Health, Social Care & Wellbeing",
          "AM90": "Housing",
          "AM100": "Money & Finance",
          "AM110": "Nature, Conservation & Environment",
          "AM120": "Reduce, Reuse, Repair & Recycle",
          "AM130": "Agriculture",
          "AM140": "Industry",
          "AM150": "Utilities",
          "AM160": "Transport"
        }
      }
    }
