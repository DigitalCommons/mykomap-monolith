    "lng": {
      "type": "value",
      "as": "number",
      "nullable": true,
      "from": "Geocoded Longitude"      
    },
    "address": {
      "type": "value",
      "filter": true,
      "from": "Address"
    },
    "activity": {
      "type": "vocab",
      "uri": "am:",
      "from": "Activity"
    },
    "otherActivities": {
      "type": "multi",
      "of": {
        "type": "vocab",
        "uri": "am:"
      },
      "from": "Other Activities"
    },
    "websites": {
      "type": "multi",
      "of": {
        "type": "value"
      },
      "from": "Websites"
    },
    "validated": {
      "type": "value",
      "as": "boolean",
      "from": "Validated"
