{
  "name": "@mykomap/back-end",
  "version": "4.1.0",
  "description": "Server API for Mykomap application clients",
  "author": "Digital Commons Cooperative",
  "homepage": "https://digitalcommons.github.io/mykomap-monolith",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "dataset": "bin/dataset"
  },
  "scripts": {
    "clean": "rimraf node_modules dist node-dist",
    "dataset": "vite-node bin/dataset.ts",
    "dataset-minimal": "vite-node bin/datasetMinimal.ts",
    "prebuild": "npm -w ../../libs/common -w ../../libs/node-utils run build",
    "build": "vite build --ssr src/index.ts --outDir dist",
    "start": "node ./start.js",
    "start:attached": "npm run start",
    "test": "cross-env vitest run --config vitest.config.ts",
    "dev": "vite-node -w start.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@immobiliarelabs/fastify-sentry": "^8.0.2",
    "@mykomap/common": "*",
    "@mykomap/node-utils": "*",
    "@seriousme/openapi-schema-validator": "^2.2.1",
    "@ts-rest/fastify": "^3.51.0",
    "clipanion": "^4.0.0-rc.4",
    "close-with-grace": "^2.1.0",
    "csv-parse": "^5.6.0",
    "diff": "^8.0.2",
    "fastify-openapi-glue": "^4.7.0",
    "fastify-plugin": "^4.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "qs": "^6.14.0",
    "tsx": "^4.19.2",
    "udsv": "^0.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@storybook/addon-a11y": "^8.3.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.5.4",
    "@types/qs": "^6.9.18",
    "c8": "^10.1.2",
    "cross-env": "^7.0.3",
    "dir-compare": "^5.0.0",
    "fastify": "^4.28.1",
    "fastify-cli": "^6.3.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.18",
    "vite-node": "^3.0.9",
    "vite-plugin-dts": "^4.1.0",
    "vitest": "^3.0.9"
  },
  "config": {
    "sentry": {
      "release": "@mykomapback-end@4.1.0"
    }
  }
}
