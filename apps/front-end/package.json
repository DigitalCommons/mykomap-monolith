{
  "name": "@mykomap/front-end",
  "version": "4.0.0",
  "description": "A React web application for mapping initiatives in the Solidarity Economy",
  "author": "Digital Commons Cooperative",
  "homepage": "https://digitalcommons.github.io/mykomap-monolith",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist node_modules",
    "prestart": "cross-env NODE_ENV=development npm -w ../back-end run build",
    "start": "cross-env NODE_ENV=development tsx --tsconfig tsconfig.node.json --watch --watch-path=src --watch-path=../back-end/src/ server.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@mykomap/common": "*",
    "@nazka/map-gl-js-spiderfy": "github:DigitalCommons/map-gl-js-spiderfy#improvements-for-mykomap",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/browser": "^8.26.0",
    "@ts-rest/core": "^3.51.0",
    "@types/geojson": "^7946.0.14",
    "maplibre-gl": "^4.5.2",
    "maplibregl-spiderfier": "github:ohrie/maplibre-spiderfier",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@fastify/vite": "^6.0.7",
    "@mykomap/back-end": "*",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.4",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "fastify": "^4.28.1",
    "gh-pages": "^6.1.1",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "rimraf": "^6.0.1",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.9",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  }
}
