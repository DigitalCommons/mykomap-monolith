{
  "name": "@mykomap/front-end",
  "version": "4.1.0",
  "description": "A React web application for mapping Solidarity Economy organisations",
  "author": "Digital Commons Cooperative",
  "homepage": "https://digitalcommons.github.io/mykomap-monolith",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist node_modules",
    "prestart": "cross-env NODE_ENV=development npm -w ../back-end run build",
    "start": "cross-env NODE_ENV=development VITE_API_URL=/api tsx watch --tsconfig tsconfig.node.json --include '../back-end/' --exclude '**/.#*' --exclude '**/vite.config.ts.timestamp*' server.ts",
    "dev": "cross-env VITE_API_URL=http://localhost:3000 vite",
    "prebuild": "npm -w ../../libs/common run build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "upload-sourcemaps": "sentry-cli sourcemaps --release $npm_package_config_sentry_release upload --url-prefix /assets/ dist/assets/"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hey-api/client-fetch": "^0.2.4",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.0.2",
    "@mykomap/common": "*",
    "@nazka/map-gl-js-spiderfy": "github:DigitalCommons/map-gl-js-spiderfy#improvements-for-mykomap",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/browser": "^8.26.0",
    "@sentry/cli": "^2.42.2",
    "@ts-rest/core": "^3.51.0",
    "@types/geojson": "^7946.0.14",
    "html-entities": "^2.5.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "maplibre-gl": "^4.5.2",
    "maplibregl-spiderfier": "github:ohrie/maplibre-spiderfier",
    "mui-markdown": "^1.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.1.1",
    "react-redux": "^9.1.0",
    "vite-node": "^3.0.9"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@fastify/vite": "^6.0.7",
    "@mykomap/back-end": "*",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-themes": "^8.3.0",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.4",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "fastify": "^4.28.1",
    "jsdom": "^23.2.0",
    "msw": "^2.6.5",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "rimraf": "^6.0.1",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.9",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.18",
    "vitest": "^3.0.9"
  },
  "config": {
    "sentry": {
      "release": "@mykomapfront-end@4.1.0"
    }
  }
}
